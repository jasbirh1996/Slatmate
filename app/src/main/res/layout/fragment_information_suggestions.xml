<?xml version="1.0" encoding="utf-8"?>


<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.dashboard.welcome.FragmentInformationSuggestions">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/lets"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Let's get started"
        android:textColor="@color/obboard_indicator"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginHorizontal="36dp"
        android:layout_marginTop="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lets"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="36dp">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/gradutionHeading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins"
            android:text="@string/graduation"
            android:textColor="@color/grey1"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/filledTextGraduation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/style_spinner"
            android:gravity="center_vertical"
            android:paddingVertical="10dp"
            android:textColor="@color/black"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/gradutionHeading"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/gradutionHeading" />


        <View
            android:id="@+id/viewGradution"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/grey1"
            app:layout_constraintTop_toBottomOf="@+id/filledTextGraduation" />


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/majorHeading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/poppins"
            android:text="@string/major_in"
            android:textColor="@color/grey1"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/viewGradution" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/filledTextMajor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/style_spinner"
            android:gravity="center_vertical"
            android:paddingVertical="10dp"
            android:textColor="@color/black"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/majorHeading"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/majorHeading" />

        <View
            android:id="@+id/viewMajor"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/grey1"
            app:layout_constraintTop_toBottomOf="@+id/filledTextMajor" />


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/countryHeading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/poppins"
            android:text="@string/country"
            android:textColor="@color/grey1"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/filledTextMajor" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/filledTextCountry"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/style_spinner"
            android:gravity="center_vertical"
            android:paddingVertical="10dp"
            android:textColor="@color/black"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/countryHeading"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/countryHeading" />


        <View
            android:id="@+id/viewCountry"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/grey1"
            app:layout_constraintTop_toBottomOf="@+id/filledTextCountry" />


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/cityHeading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/poppins"
            android:text="@string/city"
            android:textColor="@color/grey1"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/viewCountry" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/filledTextCity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/style_spinner"
            android:gravity="center_vertical"
            android:paddingVertical="10dp"
            android:textColor="@color/black"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/cityHeading"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cityHeading" />

        <View
            android:id="@+id/viewCity"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/grey1"
            app:layout_constraintTop_toBottomOf="@+id/filledTextCity" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/btn_next"
            style="@style/customButtonStyle"
            android:layout_width="290dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:gravity="center"
            android:padding="14dp"
            android:text="@string/next"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:textAllCaps="false" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>


</layout>