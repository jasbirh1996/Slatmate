<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/funtimepostnavigation"
    app:startDestination="@id/fragmentFuntimePost">

    <fragment
        android:id="@+id/fragmentFuntimePost"
        android:name="com.stalmate.user.view.dashboard.funtime.FragmentFuntimePost"
        android:label="fragmentFuntime"
        tools:layout="@layout/fragment_funtime_post">
        <action
            android:id="@+id/action_fragmentFuntimePost_to_fragmentFuntimeTag"
            app:destination="@id/fragmentFuntimeTag" />
        <action
            android:id="@+id/action_fragmentFuntimePost_to_FragmentFuntimePrivacyOptions"
            app:destination="@id/FragmentFuntimePrivacyOptions" />
        <action
            android:id="@+id/action_fragmentFuntimePost_to_fragmentSelectThumbnail"
            app:destination="@id/fragmentSelectThumbnail" />
    </fragment>

    <fragment
        android:id="@+id/fragmentFuntimeTag"
        android:name="com.stalmate.user.view.dashboard.funtime.FragmentFuntimeTag"
        android:label="fragmentTag"
        tools:layout="@layout/fragment_funtime_tag">
        <action
            android:id="@+id/action_fragmentFuntimeTag_to_fragmentSingleUserSelector"
            app:destination="@id/fragmentSingleUserSelector" />
    </fragment>


    <fragment
        android:id="@+id/fragmentSelectThumbnail"
        android:name="com.stalmate.user.view.dashboard.funtime.SelectThumbnailFragment"
        android:label="fragmentThumbnail"
        tools:layout="@layout/fragment_select_thumbnail" />

    <fragment
        android:id="@+id/fragmentSingleUserSelector"
        android:name="com.stalmate.user.view.dashboard.funtime.FragmentSingleUserSelector"
        android:label="fragmentSingleSelector"
        tools:layout="@layout/fragment_single_user_selector">

    </fragment>

    <fragment
        android:id="@+id/FragmentFuntimePrivacyOptions"
        android:name="com.stalmate.user.view.dashboard.funtime.FragmentFuntimePrivacyOptions"
        android:label="fragmentPrivacy"
        tools:layout="@layout/fragment_funtime_privacy_options">

    </fragment>


    <fragment
        android:id="@+id/FragmentFuntimePostEdit"
        android:name="com.stalmate.user.view.dashboard.funtime.FragmentFuntimePostEdit"
        android:label="fragmentpostedit"
        tools:layout="@layout/fragment_funtime_post_edit">

    </fragment>
</navigation>